{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:dev": "nodemon",
    "start": "node build/index.js",
    "copy-assets": "mkdir -p dist/src && cp -R src/templates dist/src",
    "dev": "cross-env NODE_ENV=development nodemon",
    "test": "jest",
    "transpile": "tsc",
    "clean": "rimraf dist",
    "build": "yarn clean && yarn transpile && yarn copy-assets",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "eslint --ignore-path .gitignore --ext .ts src/ --fix",
    "prettier": "prettier --ignore-path .gitignore --write \"src/**/*.ts\"",
    "prettier:check": "prettier --ignore-path .gitignore --check \"src/**/*.ts\"",
    "knex": "knex --knexfile=src/knexFile.ts",
    "seed": "yarn knex seed:run --verbose",
    "migrate": "yarn knex migrate:latest --verbose",
    "rollback": "yarn knex migrate:rollback --verbose",
    "make:seeder": "yarn knex seed:make -x ts --env development",
    "make:migration": "yarn knex migrate:make -x ts --env development"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "limiter": "^2.1.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.9.8",
    "nodemon": "^3.0.2",
    "pg": "^8.11.5",
    "prettier": "^3.1.1",
    "redis": "^4.6.14",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/knex": "^0.16.1",
    "@types/node": "^20.12.12",
    "jest": "^29.7.0",
    "morgan": "^1.10.0",
    "ts-jest": "^29.1.4",
    "typescript": "^5.4.5"
  }
}
